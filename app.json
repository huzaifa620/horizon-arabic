{
  "expo": {
    "name": "Horizon Arabic",
    "slug": "horizon-arabic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pic1.jpg",
    "scheme": "horizonarabic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.huzaifa620.horizonarabic",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0a1929",
        "foregroundImage": "./assets/images/pic1.jpg",
        "backgroundImage": "./assets/images/pic1.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.huzaifa620.horizonarabic"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/pic1.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pic1.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a1929",
          "dark": {
            "backgroundColor": "#0a1929"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2feb37c-5f26-4f6e-8b0b-a75dc8fade9e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d2feb37c-5f26-4f6e-8b0b-a75dc8fade9e"
    }
  }
}
